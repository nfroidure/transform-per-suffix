{
  "name": "transform-per-suffixes",
  "version": "1.0.0",
  "description": "Map object properties according to suffixes.",
  "main": "index.js",
  "metapak": {
    "data": {
      "files": "index.js index.mocha.js",
      "testsFiles": "index.mocha.js"
    },
    "configs": [
      "jsarch",
      "jsdocs",
      "readme"
    ]
  },
  "scripts": {
    "architecture": "jsarch index.js index.mocha.js > ARCHITECTURE.md",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "cli": "env NODE_ENV=${NODE_ENV:-cli}",
    "cover": "istanbul cover _mocha --report html -- index.mocha.js -R spec -t 5000",
    "coveralls": "istanbul cover _mocha --report lcovonly -- index.mocha.js -R spec -t 5000 && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "cz": "env NODE_ENV=${NODE_ENV:-cli} git cz",
    "doc": "mkdir -p .readme; echo \"# API\" > .readme/API.md; jsdoc2md index.js index.mocha.js >> .readme/API.md",
    "lint": "eslint index.js index.mocha.js",
    "metapak": "metapak || echo 'Please `npm install --save-dev metapak`' && exit 0",
    "postinstall": "npm run metapak; npm run metapak --silent",
    "preversion": "npm t && npm run lint",
    "test": "mocha index.mocha.js",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "keywords": [
    "map",
    "object",
    "suffix",
    "array"
  ],
  "author": "Nicolas Froidure",
  "repository": {
    "type": "git",
    "url": "https://github.com/nfroidure/transform-per-suffixes.git"
  },
  "license": "MIT",
  "devDependencies": {
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "^1.2.0",
    "coveralls": "2.11.15",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "3.16.0",
    "eslint-config-simplifield": "4.1.1",
    "istanbul": "0.4.5",
    "jsarch": "1.1.1",
    "jsdoc-to-markdown": "^3.0.0",
    "mermaid": "^7.0.0",
    "metapak": "0.0.18",
    "metapak-nfroidure": "0.5.0",
    "mocha": "3.2.0",
    "mocha-lcov-reporter": "1.3.0",
    "phantomjs": "^2.1.7"
  },
  "engines": {
    "node": ">=6.9.5"
  },
  "dependencies": {
    "debug": "2.6.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "greenkeeper": {
    "ignore": [
      "debug",
      "eslint",
      "eslint-config-simplifield",
      "mocha",
      "mocha-lcov-reporter",
      "commitizen",
      "cz-conventional-changelog",
      "coveralls",
      "istanbul",
      "conventional-changelog-cli",
      "mermaid",
      "phantomjs",
      "jsarch",
      "jsdoc-to-markdown"
    ]
  }
}
